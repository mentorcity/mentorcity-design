<div class="col-sm-6 col-md-6 col-lg-6 col-xl-3">
  <div class="primary-container__tile primary-container__tile--2">
    <p>
      Meetings Availability
      <%= display_img({src: '/assets/icons/question-mark.svg', alt: 'question-mark-icon', class: 'ml-1' }) %>

    </p>
    <div class="meeting-timebox">
      <p class="meeting-timebox__day gray">Mo, Tu, Th, Fr</p>
      <p class="meeting-timebox__time light-gray">
        7:00 AM - 10:00 PM
        <%= display_img({src: '/assets/icons/edit.svg', alt: 'edit-icon', class: 'ml-4' }) %>
      </p>
      <p class="meeting-timebox__timezone">(UTC+01:00) Europe - London</p>
    </div>
    <div class="meeting-schedule">
      <p class="meeting-schedule__title">
        <span> Your Google Calendar is connected to Scheduler. </span>
        <%= display_img({src: '/assets/icons/question-mark.svg', alt: 'question-mark-icon', class: 'mark-icon' }) %>
      </p>
    </div>
    <div class="primary-container__btn-box">
      <%= link_to 'Edit', '#', {class: 'button button--outline'} %>
      <%= link_to 'New', '#', {class: 'button button--outline'} %>
    </div>
  </div>
</div>