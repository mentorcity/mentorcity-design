<div class="tab-pane fade show active" id="achievements" role="tabpanel" aria-labelledby="achievements-tab">
  <div class="awards__cards">
    <!-- award card -->
    <div class="awards__card">
      <%= display_img ({src: '/assets/icons/awards_icons/awards_icon.svg', alt: 'awards_icon'}) %>
      <h4 class="awards__card-title">240</h4>
      <p>awards points</p>
    </div>
    <!-- award card end-->
    <div class="awards__card">
       <%= display_img ({src: '/assets/icons/awards_icons/award-connected-icon.svg', alt: 'award-connected-icon'}) %>
      <h4>Connected</h4>
      <p>with total <%= link_to '34 Members', '#' %></p>
    </div>
    <div class="awards__card">
       <%= display_img ({src: '/assets/icons/awards_icons/award-communicator-icon.svg', alt: 'communicator-icon'}) %>
      <h4>Communicator</h4>
      <p>Earned Badge</p>
    </div>
    <div class="awards__card">
       <%= display_img ({src: '/assets/icons/awards_icons/award-commencement-icon.svg', alt: 'commencement-icon'}) %>
      <h4>Commencement</h4>
      <p>Earned Badge</p>
    </div>
    <div class="awards__card">
       <%= display_img ({src: '/assets/icons/awards_icons/award-active-member-icon.svg', alt: 'award-active-member-icon'}) %>
