      <h4>Communicator</h4>
      <p>Earned Badge</p>
    </div>
    <div class="awards__card">
       <%= display_img ({src: '/assets/icons/awards_icons/award-helper-icon.svg', alt: 'award-helper-icon'}) %>
      <h4>Helper</h4>
      <p>Earned Badge</p>
    </div>
  </div>
</div>
  